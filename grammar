block = blockquote / paragraph / lines

listItem = "-" [block] *(NEWLINE INDENT [block])

blockquote = ">" [block] *(NEWLINE blockquote)

paragraph = "|" [block] *(NEWLINE paragraph)

lines = line *(NEWLINE line)
line  = 1*inline

inline = TEXT
TEXT = ; anything except NEWLINE until an inline delimiter
