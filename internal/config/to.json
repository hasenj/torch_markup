{
	"text": {
		"templates": {
			"html": "{{.Content}}"
		}
	},
	"line": {
		"templates": {
			"html": "{{if not (onlyLineComment .InlineChildren)}}<div>{{range .InlineChildren}}{{render .}}{{end}}</div>{{end}}"
		}
	},
	"lineComment": {
		"templates": {
			"html": ""
		}
	},
	"elements": [
		{
			"name": "Blockquote",
			"type": "walled",
			"delimiter": ">",
			"templates": {
				"html": "<blockquote>\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</blockquote>"
			}
		},
		{
			"name": "DescriptionList",
			"type": "hanging",
			"delimiter": "*",
			"templates": {
				"html": "<dl>\n{{- range $index, $element := .BlockChildren}}\n\t{{- if eq $index 0}}\n\t<dt>{{render $element}}</dt>\n\t{{- else}}\n\t<dd>{{render $element}}</dd>\n\t{{- end}}\n{{end -}}\n</dl>"
			}
		},
		{
			"name": "CodeBlock",
			"type": "fenced",
			"delimiter": "`",
			"templates": {
				"html": "<pre><code{{with head .Lines}} lang=\"{{.}}\"{{end}}>{{body .Lines}}</code></pre>"
			}
		},
		{
			"name": "Heading",
			"type": "hanging",
			"delimiter": "=",
			"ranked": true,
			"templates": {
				"html": "<h{{.Rank}}>\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</h{{.Rank}}>"
			}
		},
		{
			"name": "NumberedHeading",
			"type": "hanging",
			"delimiter": "#",
			"ranked": true,
			"minRank": 2,
			"templates": {
				"html": "<h{{.Rank}}>\n\t<div style=\"float:left\">{{.SeqNum}}&nbsp;</div>\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</h{{.Rank}}>"
			}
		},
		{
			"name": "Image",
			"type": "hanging",
			"delimiter": ".image",
			"verbatim": true,
			"templates": {
				"html": "<img{{with primarySecondary .Lines}}{{with .Primary}} src=\"{{.}}\"{{end}}{{with .Secondary}} alt=\"{{.}}\"{{end}}{{end}}/>"
			}
		},
		{
			"name": "NumberedListItemDot",
			"type": "hanging",
			"delimiter": "1.",
			"templates": {
				"html": "<li>{{- range .BlockChildren}}{{render .}}{{end -}}</li>"
			}
		},
		{
			"name": "NumberedListItemParen",
			"type": "hanging",
			"delimiter": "1)",
			"templates": {
				"html": "<li>{{- range .BlockChildren}}{{render .}}{{end -}}</li>"
			}
		},
		{
			"name": "ListItemDot",
			"type": "hanging",
			"delimiter": "-",
			"templates": {
				"html": "<li>{{- range .BlockChildren}}{{render .}}{{end -}}</li>"
			}
		},
		{
			"name": "TableOfContents",
			"type": "hanging",
			"delimiter": ".tableOfContents",
			"verbatim": true,
			"templates": {
				"html": "<p>Table Of Contents</p>{{.Aggregates.Headings}}"
			}
		},

		{
			"name": "Emphasis",
			"type": "uniform",
			"delimiter": "_",
			"templates": {
				"html": "<em>{{range .InlineChildren}}{{render .}}{{end}}</em>"
			}
		},
		{
			"name": "Strong",
			"type": "uniform",
			"delimiter": "*",
			"templates": {
				"html": "<strong>{{range .InlineChildren}}{{render .}}{{end}}</strong>"
			}
		},
		{
			"name": "Code",
			"type": "escaped",
			"delimiter": "`",
			"templates": {
				"html": "<code>{{.Content}}</code>"
			}
		},
		{
			"name": "Link",
			"type": "forward",
			"delimiter": "<",
			"templates": {
				"html": "<a href=\"{{.Content}}\">{{range .InlineChildren}}{{render .}}{{else}}{{.Content}}{{end}}</a>"
			}
		}
	],
	"groups": [
		{
			"name": "NumberedListDot",
			"element": "NumberedListItemDot",
			"templates": {
				"html": "<ol class=\"dot\">\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</ol>"
			}
		},
		{
			"name": "NumberedListParen",
			"element": "NumberedListItemParen",
			"templates": {
				"html": "<ol class=\"paren\">\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</ol>"
			}
		},
		{
			"name": "ListDot",
			"element": "ListItemDot",
			"templates": {
				"html": "<ul>\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</ol>"
			}
		}
	],
	"aggregates": [
		{
			"name": "Headings",
			"elements": ["NumberedHeading"]
		}
	]
}
