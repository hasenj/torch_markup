{
	"text": {
		"templates": {
			"html": "{{.Content}}"
		}
	},
	"line": {
		"templates": {
			"html": "{{if not (onlyLineComment .InlineChildren)}}<div>{{range .InlineChildren}}{{render .}}{{end}}</div>{{end}}"
		}
	},
	"elements": [
		{
			"name": "Blockquote",
			"type": "walled",
			"delimiter": ">",
			"templates": {
				"html": "<blockquote>\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</blockquote>"
			}
		},
		{
			"name": "DescriptionList",
			"type": "hanging",
			"delimiter": "*",
			"templates": {
				"html": "<dl>\n{{- range $index, $element := .BlockChildren}}\n\t{{- if eq $index 0}}\n\t<dt>{{render $element}}</dt>\n\t{{- else}}\n\t<dd>{{render $element}}</dd>\n\t{{- end}}\n{{end -}}\n</dl>"
			}
		},
		{
			"name": "CodeBlock",
			"type": "fenced",
			"delimiter": "`",
			"templates": {
				"html": "<pre><code>{{.Body}}</code></pre>"
			}
		},
		{
			"name": "Heading",
			"type": "hanging",
			"delimiter": "=",
			"ranked": true,
			"templates": {
				"html": "<h{{.Rank}}>\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</h{{.Rank}}>"
			}
		},
		{
			"name": "NumberedHeading",
			"type": "hanging",
			"delimiter": "#",
			"ranked": true,
			"minRank": 2,
			"templates": {
				"html": "<h{{.Rank}}>\n\t<div style=\"float:left\">{{.SeqNum}}&nbsp;</div>\n{{- range .BlockChildren}}\n\t{{render .}}\n{{end -}}\n</h{{.Rank}}>"
			}
		},
		{
			"name": "Replaced",
			"type": "hanging",
			"delimiter": ".",
			"leaf": true,
			"templates": {
				"html": "<img />{{range .BlockChildren}}{{render .}}{{end}}"
			}
		},
		{
			"name": "NumberedListDot",
			"type": "hanging",
			"delimiter": "1.",
			"templates": {
				"html": "<ol>\n{{- range .BlockChildren}}\n\t<li>{{render .}}</li>\n{{end -}}\n</ol>"
			}
		},
		{
			"name": "NumberedListParen",
			"type": "hanging",
			"delimiter": "1)",
			"templates": {
				"html": "<ol class=\"paren\">\n{{- range .BlockChildren}}\n\t<li>{{render .}}</li>\n{{end -}}\n</ol>"
			}
		},

		{
			"name": "Emphasis",
			"type": "uniform",
			"delimiter": "_",
			"templates": {
				"html": "<em>{{range .InlineChildren}}{{render .}}{{end}}</em>"
			}
		},
		{
			"name": "Strong",
			"type": "uniform",
			"delimiter": "*",
			"templates": {
				"html": "<strong>{{range .InlineChildren}}{{render .}}{{end}}</strong>"
			}
		},
		{
			"name": "Code",
			"type": "escaped",
			"delimiter": "`",
			"templates": {
				"html": "<code>{{.Content}}</code>"
			}
		},
		{
			"name": "Link",
			"type": "forward",
			"delimiter": "<",
			"templates": {
				"html": "<a href=\"{{.Content}}\">{{range .InlineChildren}}{{render .}}{{else}}{{.Content}}{{end}}</a>"
			}
		}
	]
}
